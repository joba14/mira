
#ifndef __factorial_mira__
#define __factorial_mira__

#include "./../std/posix.mira"
#include "./../std/io.mira"

sec .text func factorial req u64 ret u64 {
	if [ dup 0 <= ] {
		0
	}
	else {
		1 swap loop [ dup 0 > ] {
			swap over * swap --
		} drop
	}
}

sec .text func _start {
	0 loop [ dup 10 < ] {
		dup as u64 call factorial
		stdout call fputu
		10 stdout call fputc
		++
	} drop

	0 call exit
}

#endif
