
#define width 11
#define height 10
#define N 110
sec .bss mem grid N
sec .text func print-square {
	0 loop [ dup height < ] {
		0 loop [ dup width 1 - < ] {
			swap over over height * + grid + 35 as u08 swap st08 swap ++
		} drop

		dup 1 + height * 1 - grid + 10 as u08 swap st08 ++
	} drop

	N grid 1 1 sys3 drop
}

sec .text func _start {
	call print-square
	0 60 sys1 drop
}
