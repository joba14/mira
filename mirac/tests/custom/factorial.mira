
func putu { }

func factorial1
	req u64
	ret u64
{
	\+. \+. 0 > loop {
		1 -
		\>< \>. * \><
		\+. 0 >
	} \!.
}

func factorial2
	req u64
	ret u64
{
	\+. 1 < if {
		\!. 1
	} else {
		\+. 1 - factorial2 *
	}
}

func main {
	4 factorial1 putu
	5 factorial1 putu
	6 factorial1 putu

	4 factorial2 putu
	5 factorial2 putu
	6 factorial2 putu
}
