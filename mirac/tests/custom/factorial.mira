
func factorial1
	req u64
	ret u64
{
	\+. loop \+. 0 > {
		1 -
		\>< \>. * \><
	} \!.
}

func factorial2
	req u64
	ret u64
{
	if \+. 1 < {
		\!. 1
	} else {
		\+. 1 - factorial2 *
	}
}

func main {
	4 factorial1 putu
	5 factorial1 putu
	6 factorial1 putu

	4 factorial2 putu
	5 factorial2 putu
	6 factorial2 putu
}
