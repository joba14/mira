
func factorial1
	req u64
	ret u64
do
	dup loop dup 0 > do
		1 -
		swap over * swap
	end drop
end

func factorial2
	req u64
	ret u64
do
	if dup 1 < do
		drop 1
	else
		dup 1 - factorial2 *
	end
end

func main do
	4 factorial1 putu
	5 factorial1 putu
	6 factorial1 putu

	4 factorial2 putu
	5 factorial2 putu
	6 factorial2 putu
end
